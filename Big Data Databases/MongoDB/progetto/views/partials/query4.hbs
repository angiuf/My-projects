<!-- 4 QUERY -->
<div class="col-sm">
    <div class="card mb-2">
        <div class="card-header" style="background-color: #52796f; color: white; font-weight: bold">
            4. Find all people that are both vaccinated and have done at least once a covid test.
        </div>
        <div class="card-body">
            <div class="input-group">
                <div class="container-fluid p-0 mb-2">
                    Click on the button "Calculate" to find the number of people that are vaccinated but have done the covid-test at least once.
                </div>
                <form class="form-inline mb-0" action="/q4" method="POST">
                    <div class="input-group-append">
                        <button type="submit" class="btn btn-outline-secondary">Calculate</button>
                    </div>
                </form>
            </div>
            {{#if post.outDiv_query4}}
                <div class="container-fluid mt-2 px-1 bg-light border border-2 rounded" style="display: block">
                    {{breaklines post.outDiv_query4}}
                </div>
            {{/if}}
        </div>
    </div>
</div>
